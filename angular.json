{
  "version": 1,
  "projects": {
    "my-app": {
      "projectType": "application",
      "root": "apps/my-app",
      "sourceRoot": "apps/my-app/src",
      "prefix": "my-workspace",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/my-app",
            "index": "apps/my-app/src/index.html",
            "main": "apps/my-app/src/main.ts",
            "polyfills": "apps/my-app/src/polyfills.ts",
            "tsConfig": "apps/my-app/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/my-app/src/favicon.ico", "apps/my-app/src/assets"],
            "styles": ["apps/my-app/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/my-app/src/environments/environment.ts",
                  "with": "apps/my-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "my-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "my-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "my-app:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/my-app/src/**/*.ts",
              "apps/my-app/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/my-app"],
          "options": {
            "jestConfig": "apps/my-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "my-app-e2e": {
      "root": "apps/my-app-e2e",
      "sourceRoot": "apps/my-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/my-app-e2e/cypress.json",
            "tsConfig": "apps/my-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "my-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "my-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/my-app-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "web-components": {
      "projectType": "library",
      "schematics": {
        "@nxext/stencil:component": {
          "style": "scss",
          "storybook": false
        }
      },
      "root": "libs/web-components",
      "sourceRoot": "libs/web-components/src",
      "architect": {
        "test": {
          "builder": "@nxext/stencil:test",
          "options": {
            "projectType": "library",
            "configPath": "libs/web-components/stencil.config.ts"
          }
        },
        "e2e": {
          "builder": "@nxext/stencil:e2e",
          "options": {
            "projectType": "library",
            "configPath": "libs/web-components/stencil.config.ts"
          }
        },
        "build": {
          "builder": "@nxext/stencil:build",
          "options": {
            "projectType": "library",
            "configPath": "libs/web-components/stencil.config.ts"
          }
        },
        "serve": {
          "builder": "@nxext/stencil:build",
          "options": {
            "projectType": "library",
            "configPath": "libs/web-components/stencil.config.ts",
            "serve": true,
            "watch": true
          }
        }
      }
    },
    "web-components-angular": {
      "projectType": "library",
      "root": "libs/web-components-angular",
      "sourceRoot": "libs/web-components-angular/src",
      "prefix": "my-workspace",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/web-components-angular/tsconfig.lib.json",
              "libs/web-components-angular/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/web-components-angular/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/web-components-angular"],
          "options": {
            "jestConfig": "libs/web-components-angular/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "lib-a": {
      "root": "libs/lib-a",
      "sourceRoot": "libs/lib-a/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/lib-a/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/lib-a"],
          "options": {
            "jestConfig": "libs/lib-a/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "my-app"
}
